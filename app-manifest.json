{
  "name": "Trip Planner",
  "version": "0.1",
  "manifest_version": 2,
  "description": "A trip planner HTML5 app",
  "icons": {                     // recommended
    "128": "trip-128.png" // recommended
  },
  "update_url": "http://rt24.intel.com/check_update",
  "app": {
    "launch": {
      "local_path": "index.html"
      "container": "window"
    }
  },
  "permissions": ["http://*.dropbox.com/","http://*.dropbox.com/"]
}
